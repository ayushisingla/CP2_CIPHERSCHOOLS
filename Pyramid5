import java.util.*;
public class Pyramid5 
{ 

	
	public static void print(int n) 
	{ 
		int i, j, num=1; 
		for(i=0; i<n; i++) 
		{ 
			for(j=0; j<=i; j++) 
			{ 
				System.out.print(num+ " "); 
				num = num + 1; 
			} 
			System.out.println(); 
		} 
	} 
	public static void main(String args[]) 
	{ 
        Scanner sc = new Scanner(System.in); 
		print(sc.nextInt());  
	} 
} 
